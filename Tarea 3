              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "# Adapted from Simpy companion\n",
        "!pip install simpy\n",
        "import simpy\n",
        "import random\n",
        "from scipy.stats import expon\n",
        "import statistics\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "arrival_times = []\n",
        "system_times = []\n",
        "\n",
        "class Theater(object):\n",
        "    # inicializamos la simulación, definiendo los recursos\n",
        "    def __init__(self, env, num_cashiers, num_servers, num_ushers):\n",
        "        # ambiente de simulación\n",
        "        self.env = env\n",
        "        # recurso cajeros\n",
        "        self.cashier = simpy.Resource(env, num_cashiers)\n",
        "        # recurso atención comidas\n",
        "        self.server = simpy.Resource(env, num_servers)\n",
        "        # recurso chequeo de boletas\n",
        "        self.usher = simpy.Resource(env, num_ushers)\n",
        "\n",
        "    # definimos el proceso de compra de boletas\n",
        "    def purchase_ticket(self, moviegoer):\n",
        "        # el tiempo de compra de boletas se distribuye exponencial con media 2 minutos\n",
        "        yield self.env.timeout(expon.rvs(2))\n",
        "\n",
        "    # definimos el proceso de compra de comida\n",
        "    def sell_food(self, moviegoer):\n",
        "        # el tiempo de compra de comida se distribuye exponencial con media 4 minutos\n",
        "        yield self.env.timeout(expon.rvs(4))\n",
        "\n",
        "    # definimos el proceso de chequeo de boletas\n",
        "    def check_ticket(self, moviegoer):\n",
        "        # el tiempo de compra de chequeo de boletas es constante y dura 20 segundos\n",
        "        yield self.env.timeout(20 / 60)\n",
        "\n",
        "\n",
        "# definimos el proceso de visita de un cliente al cine\n",
        "def go_to_movies(env, moviegoer, theater):\n",
        "    # almacenamos el momento en que el cliente llega el cine\n",
        "    arrival_time = env.now\n",
        "    arrival_times.append(arrival_time)\n",
        "\n",
        "    # el cliente compra la boleta\n",
        "    # agregar un parametro que espcifique que el 30 % ya tienen boletas\n",
        "    with theater.cashier.request() as request:\n",
        "        yield request\n",
        "        yield env.process(theater.purchase_ticket(moviegoer))\n",
        "\n",
        "    # con probabilidad 1/2 el cliente compra comida\n",
        "    if random.choice([True, False]):\n",
        "        with theater.server.request() as request:\n",
        "            yield request\n",
        "            yield env.process(theater.sell_food(moviegoer))\n",
        "\n",
        "    # al cliente se le revisa la boleta\n",
        "    with theater.usher.request() as request:\n",
        "        yield request\n",
        "        yield env.process(theater.check_ticket(moviegoer))\n",
        "\n",
        "    # Almacenamos el tiempo de salida (entrada al teatro) - tiempo de llegada\n",
        "    # Éste es el tiempo total en el sistema\n",
        "    system_times.append(env.now - arrival_time)\n",
        "\n",
        "\n",
        "# Proceso de ejecución del cine\n",
        "def run_theater(env, num_cashiers, num_servers, num_ushers):\n",
        "    theater = Theater(env, num_cashiers, num_servers, num_ushers)\n",
        "\n",
        "    # generación de arribos\n",
        "    moviegoer = 0 # contador de arribos de clientes\n",
        "    while True:\n",
        "        # tiempo entre arribos sigue una distribución exponencial con media 0.5 minutos\n",
        "        yield env.timeout(expon.rvs(0.5))\n",
        "\n",
        "        moviegoer += 1\n",
        "        env.process(go_to_movies(env, moviegoer, theater))\n",
        "\n",
        "# calcula el tiempo medio en el sistema\n",
        "def get_average_system_time(times):\n",
        "    mean_time = statistics.mean(times)\n",
        "    # Pretty print the results\n",
        "    minutes, frac_minutes = divmod(mean_time, 1)\n",
        "    seconds = frac_minutes * 60\n",
        "    return round(minutes), round(seconds)\n",
        "\n",
        "\n",
        "# método principal\n",
        "def main():\n",
        "    # Setup\n",
        "    random.seed(42)\n",
        "    num_cashiers = 3\n",
        "    num_servers = 5\n",
        "    num_ushers = 2\n",
        "\n",
        "    # Definir el ambiente de simulación\n",
        "    env = simpy.Environment()\n",
        "    # Ejecutar la simulación\n",
        "    env.process(run_theater(env, num_cashiers, num_servers, num_ushers))\n",
        "    # Definir el tiempo total de simulación\n",
        "    env.run(until=180)\n",
        "\n",
        "    # Imprimir tiempo medio en el sistema\n",
        "    mins, secs = get_average_system_time(system_times)\n",
        "    print(\n",
        "        \"Running simulation...\",\n",
        "        f\"\\nThe average wait time is {mins} minutes and {secs} seconds.\",\n",
        "    )\n",
        "\n",
        "    # guardar los resultados\n",
        "    df = pd.DataFrame()\n",
        "    df['system_time'] = system_times\n",
        "    df['arrival_time'] = arrival_times[0:len(system_times)]\n",
        "    df.to_csv(\"sim_times.csv\")\n",
        "\n",
        "    # Mostrar los resultados: tiempos en el sistema en función del tiempo de arribo\n",
        "    df.plot(x='arrival_time', y='system_time')\n",
        "    plt.xlabel('arrival time', fontsize=20)\n",
        "    plt.ylabel('time in system', fontsize=20)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "    # Mostrar los resultados: histograma de tiempos en el sistema\n",
        "    df['system_time'].hist()\n",
        "    plt.xlabel('time in system', fontsize=20)\n",
        "    plt.ylabel('frequency', fontsize=20)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
    },
